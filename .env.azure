# Azure Environment Variables
# Copy this to .env and update with your Azure credentials

# Azure Data Lake Storage Gen2
ADLS_ACCOUNT_NAME=your_adls_account_name
ADLS_ACCOUNT_KEY=your_adls_account_key
ADLS_CONTAINER_NAME=spotify-data

# Azure Service Principal (recommended for production)
AZURE_CLIENT_ID=your_client_id
AZURE_CLIENT_SECRET=your_client_secret
AZURE_TENANT_ID=your_tenant_id
AZURE_SUBSCRIPTION_ID=your_subscription_id

# Databricks
DATABRICKS_WORKSPACE_URL=your_workspace.databricks.azure.com
DATABRICKS_ACCESS_TOKEN=your_databricks_token
DATABRICKS_CLUSTER_ID=your_cluster_id
DATABRICKS_WAREHOUSE_ID=your_warehouse_id

# Azure Kafka (Event Hubs)
AZURE_EVENTHUBS_NAMESPACE=your_eventhubs_namespace
AZURE_EVENTHUBS_CONNECTION_STRING=your_connection_string

# PostgreSQL (can be Azure Database for PostgreSQL)
DB_HOST=your_postgres_host.postgres.database.azure.com
DB_PORT=5432
DB_NAME=spotify_data
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# Airflow (can be Azure Container Instances or AKS)
AIRFLOW_UID=50000
AIRFLOW_GID=0
AIRFLOW__CORE__EXECUTOR=CeleryExecutor
AIRFLOW__CORE__SQL_ALCHEMY_CONN=postgresql+psycopg2://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/airflow
AIRFLOW__CELERY__RESULT_BACKEND=db+postgresql://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/airflow
AIRFLOW__CELERY__BROKER_URL=redis://redis:6379/0

# Resource tagging
ENVIRONMENT=dev
PROJECT_NAME=spotify-pipeline
OWNER=data-engineering-team